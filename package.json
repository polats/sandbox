{
  "name": "@polats/sandbox",
  "version": "0.1.5",
  "description": "A sandbox library for import in observablehq.",
  "author": "Paul Gadi <paul@outplay.games>",
  "repository": "github:polats/sandbox",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/sandbox.esm.js",
  "unpkg": "dist/sandbox.umd.js",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.0",
    "husky": "^4.3.0",
    "size-limit": "^4.9.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.1.2"
  },
  "peerDependencies": {
    "phaser-ce": "2.10.3"
  },
  "dependencies": {
    "eventemitter3": "^4.0.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/sandbox.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/sandbox.esm.js",
      "limit": "10 KB"
    }
  ]
}
